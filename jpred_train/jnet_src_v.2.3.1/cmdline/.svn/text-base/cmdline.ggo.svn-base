package "Jnet"
# The version numbering scheme will be <major code revisions>.<updated training data>.<updated database/bug fixes>
version "2.3.1"
purpose "A consensus neural network protein secondary structure prediction method\n- Trained with SCOP 2.04 data and UniRef90 release 2014_07, 09-Jul-2014"

# These are the data input parameters - only hmmer is required
option   "pssm"         p  "PSIBLAST PSSM"    string  typestr="filename"  optional
option   "hmmer"        H  "HMMER profile"    string  typestr="filename"  required

# Program output, mutually exclusive
defgroup    "output"       groupdesc="Output options"
groupoption "concise"   -  "concise output (default)"   group="output"
groupoption "human"     -  "human readable output. Optionally, can take a width argument [default: 60]"   int   group="output" argoptional
